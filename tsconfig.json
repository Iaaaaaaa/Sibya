{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "api/webhooks/route.js",
    "lib/modal/User.js",
    "lib/modal/Post.js",
    "lib/mongodb/mongoose.js",
    "api/webhooks/route.js",
    "api/webhooks/route.js",
    "api/webhooks/route.js",
    "api/webhooks/route.js",
    "middleware.ts",
    "middleware.ts",
    "lib/actions/user.js",
    "api/webhooks/route.js",
    "middleware.ts"
  ],
  "exclude": ["node_modules"]
}
